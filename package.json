{
  "name": "article-browser",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1",
    "react": "^18",
    "react-dom": "^18"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.ts*\" && prettier \"src/**/*.ts*\" --list-different",
    "lint:fix": "eslint --fix \"src/**/*.ts*\"",
    "format": "prettier \"src/**/*.ts*\" --write",
    "build:css": "postcss src/styles/app.css -o src/app.css",
    "watch:css": "postcss src/styles/app.css -o src/app.css -w",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5",
    "@testing-library/react": "^13",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/eslint-plugin-tslint": "^5",
    "@vitejs/plugin-react": "^3",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-airbnb": "^19",
    "eslint-config-prettier": "^8",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-react": "^7",
    "postcss": "^8",
    "postcss-preset-env": "^7",
    "prettier": "^2",
    "tailwindcss": "^3",
    "tslint": "^6",
    "tslint-react": "^5",
    "typescript": "^4",
    "vite": "^4"
  }
}
